.flex-2-item{
    display: flex;
    justify-content:center;
    font-size: 30rpx;
    text-align: center;
    font-weight:300;
    line-height:50rpx;
}

.item{
    width: 42%;
    margin: 20rpx auto;
    box-shadow:0px 0px 10px 0px rgba(0,0,0,0.5);
    border-radius:15rpx;
    padding:4rpx;
}

.card image{
    width: 100%;
    height: 200rpx;
    border-radius: 20rpx
}